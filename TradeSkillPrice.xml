<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

<Button name="TSPButtonTemplate" hidden="false" virtual="true">
    <Size x="23" y="16"/>

    <Layers>
        <Layer level="OVERLAY">
            <FontString parentKey="Text" inherits="GameFontNormalSmall" justifyH="RIGHT" setAllPoints="true">
                <Color r="1" g="1" b="1" a="1" />
            </FontString>
        </Layer>
    </Layers>

    <NormalFont inherits="GameFontNormalSmall" justifyH="RIGHT"/>
    <HighlightFont inherits="GameFontHighlightSmall" justifyH="RIGHT"/>
    <DisabledFont inherits="GameFontDisableSmall" justifyH="RIGHT"/>
</Button>

</Ui>
